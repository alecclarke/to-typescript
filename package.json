{
  "name": "to-typescript",
  "displayName": "toTypeScript()",
  "description": "Converts JavaScript and CoffeeScript to TypeScript",
  "author": "Alec Clarke",
  "publisher": "alecclarkedev",
  "version": "0.1.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.33.0"
  },
  "icon": "images/icon.png",
  "homepage": "https://github.com/alecclarke/to-typescript",
  "bugs": {
    "url": "https://github.com/alecclarke/to-typescript/issues"
  },
  "repository": {
    "url": "https://github.com/alecclarke/to-typescript"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "typescript",
    "coffeescript",
    "javascript",
    "code fix",
    "code refactor"
  ],
  "activationEvents": [
    "onCommand:extension.toTypeScript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toTypeScript",
        "title": "toTypeScript()"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "toTypeScript configuration",
      "properties": {
        "toTypeScript.inferFromUsage": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.convertToAsyncFunction": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.addMissingMember": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.forgottenThisPropertyAccess": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixAwaitInSyncFunction": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixUnreachableCode": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixUnusedLabel": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.requireInTs": {
          "type": "boolean",
          "default": true,
          "description": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.0",
    "@types/prettier": "^1.19.0",
    "@types/vscode": "^1.33.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "prettier": "1.19.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "decaffeinate": "^6.0.9",
    "ts-morph": "^7.1.1"
  }
}
