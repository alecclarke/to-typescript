{
  "name": "to-typescript",
  "displayName": "toTypeScript()",
  "description": "Converts JavaScript and CoffeeScript to TypeScript",
  "author": "Alec Clarke",
  "publisher": "alecclarkedev",
  "version": "0.3.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.0"
  },
  "icon": "images/icon.png",
  "homepage": "https://github.com/alecclarke/to-typescript",
  "bugs": {
    "url": "https://github.com/alecclarke/to-typescript/issues"
  },
  "repository": {
    "url": "https://github.com/alecclarke/to-typescript"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "typescript",
    "coffeescript",
    "javascript",
    "code fix",
    "code refactor"
  ],
  "activationEvents": [
    "onCommand:extension.toTypeScript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toTypeScript",
        "title": "toTypeScript()"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "toTypeScript configuration",
      "properties": {
        "toTypeScript.inferFromUsage": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.convertToAsyncFunction": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.addMissingMember": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.forgottenThisPropertyAccess": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixAwaitInSyncFunction": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixExpectedComma": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixUnreachableCode": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.fixUnusedLabel": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.requireInTs": {
          "type": "boolean",
          "default": true,
          "description": ""
        },
        "toTypeScript.coffeescript.useCS2": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.literate": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.disableSuggestionComment": {
          "type": [
            "boolean",
            "null"
          ],
          "default": true,
          "description": ""
        },
        "toTypeScript.coffeescript.noArrayIncludes": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.useJSModules": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseJSModules": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.safeImportFunctionIdentifiers": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.preferLet": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.loose": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseDefaultParams": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseForExpressions": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseForOf": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseIncludes": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.looseComparisonNegation": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.disallowInvalidConstructors": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        },
        "toTypeScript.coffeescript.optionalChaining": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "description": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.2",
    "@types/mocha": "^8.0.3",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.0.1",
    "@types/vscode": "^1.47.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "prettier": "1.19.1",
    "typescript": "^4.3.4",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "decaffeinate": "^6.1.9",
    "ts-morph": "^9.1.0"
  }
}
